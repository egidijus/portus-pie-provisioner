
# PORTUS
RAILS_ENV=production
#RAILS_ENV=development
PORTUS_USE_SSL=yes
PORTUS_KEY_PATH=/portus/config/ssl_key.key
PORTUS_MACHINE_FQDN=portus.yourdomain.com
PORTUS_PASSWORD=###
PORTUS_SECRET_KEY_BASE=####LONGHASH####
PORTUS_PRODUCTION_HOST=db
PORTUS_PRODUCTION_USERNAME=root
PORTUS_PRODUCTION_PASSWORD=portus
