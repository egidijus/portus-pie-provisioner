# REGISTRY v2
DOCKER_HOST=172.17.0.1
PO_REGISTRY_LOG=info
PO_REGISTRY_DATA=/registry_data
PO_REGISTRY_BIND_ADDRESS=0.0.0.0
PO_REGISTRY_BIND_PORT=5000
PO_REGSITRY_DNS_NAME=docker.yourdomain.com
PO_REGISTRY_AUTH_REALM=https://portus.yourdomain.com/v2/token
PO_REGISTRY_AUTH_SERVICE=docker.yourdomain.com
PO_REGISTRY_AUTH_CERT_ISSUER=portus.yourdomain.com
PO_REGISTRY_AUTH_CERT=/etc/docker-registry/ssl_crt.crt
PO_REGISTRY_NOTIFICATIONS_NAME=portus
PO_REGISTRY_NOTIFICATIONS_URL=https://portus.yourdomain.com/v2/webhooks/events
#GODEBUG=netdns=cgo

PO_REGISTRY_AWS_ID=###AWSID###
PO_REGISTRY_AWS_KEY=###AWSSECRET###
PO_REGISTRY_AWS_REGION=eu-west-1
PO_REGISTRY_AWS_BUCKET=registry2-test
PO_REGISTRY_AWS_DIRECTORY=/images
PO_REGISTRY_AWS_CHUNK=5242880
PO_REGISTRY_DISABLE_REDIRECT=false



